// https://drafts.csswg.org/scroll-animations-1/rewrite#viewtimeline-interface

dictionary ViewTimelineOptions {
    Element subject;
    ScrollDirection axis = "block";
    // DOMString is for keyword "auto".
    sequence<(DOMString or CSSNumericValue)> inset = [];
};
